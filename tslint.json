{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning"
        },
        "semicolon": false,
        "quotemark": {
            "options": "single"
        },
        "trailing-comma": false,
        "object-literal-key-quotes": false,
        "arrow-parens": false
    }
}